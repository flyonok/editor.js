  position:relative;
  display: flex;
  /*padding: 5px 10px;*/
  /* font-size: 12px; */
  /*margin: 20px;*/
  margin-top:5px;
  margin-bottom:20px;
}
.mmxModelTable input[type=text],
.mmxModelTable label {
  margin-left: 5px;
  text-align: center;
  align-self: center;
  overflow: hidden;
  word-break: keep-all;
  /* font-weight: bold;
  font-size:16px; */
  /*border: 5px solid #0f0;*/
}
.mmxModelTable button {
  /* margin-left: 10px; */
  font-size: 10px;
  margin-left: 5px;
}
.mmxModelTable label:first-child {
  font-weight: bold;
  font-size:16px;
}
.mmxModelTable input[type=text] {
  /* border: 3px solid #0f0; */
  border:none;
  outline:none;
  position:relative;
  top:0;
  left:0;
  display:block;
  background:transparent;
  z-index:2;
  border-bottom:1px solid #808080;
  text-indent:5px;
  font-size:14px;
}
.mmxModelTable input[type=button] {
  margin-left: 100px;
  /* font-size: 14px; */
  /* border: none; */
  font-size: 10px;
  /* margin-top: 5px; */
  margin-right: 5px;
  /* margin-bottom: 5px; */
}

.mmxModelNoDisplay {
  display:none;
}
