.mmxModelGroup:before,
.mmxModelGroup:after {
  content: "";
  display: table;
}
.mmxModelGroup:after {
  clear: both;
}

.mmxConfigAttrButton {
  border: none;
  text-decoration: none;
  color: #474ea3;
  cursor: pointer;
  background-color: #ede9e9;
  z-index: 10000;
  margin-top: 20px;
  margin-bottom: 10px;
  position: relative;
  font-size: 24px !important;
}